{
  "name": "crud-service",
  "version": "1.0.0",
  "description": "Generyczny serwis CRUD z, Sequelize, PostgreSQL i TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node src/app.ts",
    "generate:schema": "ts-node generateSchema.ts",
    "generate:migrations": "ts-node generateMigrations.ts",
    "generate:migrations:custom": "node generateMigrations.js <customModelsPath>"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "pg": "^8.12.0",
    "pluralize": "^8.0.0",
    "sequelize": "^6.37.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/pluralize": "^0.0.33",
    "@types/uuid": "^10.0.0"
  },
  "license": "GPL-3.0"
}
